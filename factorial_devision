first_number = int(input())
second_number = int(input())


def factorial_numbers(numbers):
    sum = 1
    for number in range (1, numbers+1):
        sum *= number
    return sum


first_sum = factorial_numbers(first_number)
second_sum = factorial_numbers(second_number)
result = first_sum / second_sum
print(f'{result:.2f}')





