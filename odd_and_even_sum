def odd_and_even_sum(numbers):
    even_numbers_sum = 0
    odd_numbers_sum = 0
    for digit in numbers:
        if int(digit) % 2 == 0:
            even_numbers_sum += int(digit)
        else:
            odd_numbers_sum += int(digit)
    return even_numbers_sum, odd_numbers_sum


number = input()
even_numbers_sum , odd_number_sum = odd_and_even_sum(number)
print(f"Odd sum = {odd_number_sum}, Even sum = {even_numbers_sum}")
